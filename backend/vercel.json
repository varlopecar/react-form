{
  "version": 2,
  "builds": [
    {
      "src": "vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel.py"
    }
  ],
  "env": {
    "MYSQL_DATABASE": "user_registration",
    "MYSQL_USER": "user",
    "MYSQL_PASSWORD": "password",
    "MYSQL_ROOT_PASSWORD": "root",
    "MYSQL_HOST": "mysql",
    "PORT": "8000",
    "ADMIN_EMAIL": "admin@example.com",
    "ADMIN_PASSWORD": "admin123",
    "JWT_SECRET": "your-secret-key-here",
    "CORS_ORIGINS": "http://localhost:3000,http://localhost:5173,https://varlopecar.github.io,https://python-api.vercel.app"
  }
} 